% layout 'default';
%
% title l('Projects');

<!--
    Projects
-->

<div class="repoList">
    
    % for my $p ( @$projects )
    % {
        <div class="item">
            <a href="<%= url_for project_read => url => $p->{url} %>">
                <%= $p->{title} %></a> (<%= $p->{repos} %>)
        </div>
    % }
    
</div>

