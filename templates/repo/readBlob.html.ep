% layout 'default';
%
% title "/$project/" . $repo->{url} . ".git " . substr($object, 0, 10);
% stash ( tab => 'read' );
% include 'repo/vertical_nav';
                
<article>
    <table class="lines" border=0px rules="cols">
        % my $line = 0;
        % for my $l ( @$blob )
        % {
            <tr class="line">
                <td><%= ++$line; %></td>
                <td class="text"><%= $l %></td>
            </tr>
        % }
    </table>
</article>

